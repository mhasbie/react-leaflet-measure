{
  "name": "react-leaflet-measure",
  "version": "0.1.2",
  "description": "React wrapper of leaflet-measure. Coordinate, linear, and area measure control for Leaflet maps.",
  "main": "dist/react-leaflet-measure.js",
  "scripts": {
    "build": "npm run build:lib && npm run build:dist",
    "build:dist": "webpack src/index.js dist/react-leaflet-measure.js & webpack -p src/index.js dist/react-leaflet-measure.min.js",
    "build:lib": "npm run compile:lib",
    "clean:lib": "rm -Rf ./lib",
    "compile:lib": "babel src --out-dir lib",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mhasbie/react-leaflet-measure.git"
  },
  "keywords": [
    "react",
    "leaflet",
    "react-leaflet",
    "leaflet-measure",
    "react-leaflet-measure"
  ],
  "author": "M. Hasbie",
  "license": "MIT",
  "peerDependencies": {
    "leaflet": "^1.0.3",
    "react": "^15.0.0 || ^16.0.0",
    "react-leaflet": "^1.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-core": "^6.8.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.0.15",
    "babel-preset-stage-0": "^6.1.18",
    "babel-register": "^6.26.0",
    "eslint": "^3.8.1",
    "eslint-plugin-react": "^6.4.1",
    "leaflet": "^1.0.2",
    "lodash.isequal": "^4.4.0",
    "prop-types": "^15.5.10",
    "react": "^15.5.0",
    "react-dom": "^15.5.0",
    "react-leaflet": "^1.0.1",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.12.1"
  },
  "dependencies": {
    "leaflet-measure": "^3.0.2"
  }
}
